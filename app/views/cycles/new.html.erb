<%= form_for(@cycle, url: project_cycles_path(params[:project_id])) do |f| %>
  
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :duration_days %><br>
  <%= f.number_field :duration_days %><br>

  <%= f.label :start %><br>
  <%= f.date_field :start %><br>

