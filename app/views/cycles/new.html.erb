  <% valid_seeds = Array.new %>
  <% Seed.all.each do |seed %>
    <% if (seed.min_duration + seed.max_duration) / 2 <= :duration_days %>
      <% valid_seeds.push(seed) %>
    <% end %>
  <% end %>
  <% valid_seeds.all.pluck(:name).each do |seed| %>
    <%= check_box_tag "seed_names[]", seed %>
    <%= seed %>
  <% end %><br>
  
  <%= f.submit %>

<% end %>

